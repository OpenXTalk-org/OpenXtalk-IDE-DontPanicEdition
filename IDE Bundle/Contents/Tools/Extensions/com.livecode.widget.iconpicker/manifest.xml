<package version="0.0">
  <name>com.livecode.widget.iconpicker</name>
  <title>Icon Picker</title>
  <author>LiveCode</author>
  <version>1.0.3</version>
  <license>community</license>
  <type>widget</type>
  <requires name="com.livecode.library.iconsvg"/>
